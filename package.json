{
  "scripts": {
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun run db/migrate.ts",
    "db:seed": "bun run db/seed/seed.dev.ts",
    "db:init": "bun db:generate && bun db:migrate && bun db:seed",
    "db:drop": "drizzle-kit drop",
    "db:reset": "bun db:drop && rm -r sewapesta.db && bun db:generate && bun db:migrate && bun db:seed",
    "test:only": "bun test --only",
    "test:all": "bun test",
    "route:list": "bun run src/routeLists.ts",
    "dev": "bun run --hot src/index.ts"
  },
  "dependencies": {
    "@hono/zod-openapi": "^0.12.2",
    "@scalar/hono-api-reference": "^0.5.183",
    "dayjs": "^1.11.13",
    "drizzle-orm": "^0.40.0",
    "drizzle-zod": "^0.5.1",
    "hono": "^4.7.4",
    "pino": "^9.6.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bun": "latest",
    "@types/formidable": "^3.4.5",
    "drizzle-kit": "0.30.5",
    "pino-pretty": "^13.0.0"
  }
}